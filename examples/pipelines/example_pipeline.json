{
    "pipeline_name": "ObjectClassifier",
    "processors": [
	{
	    "processor_name": "Camera",
	    "processor_type": "Camera",
	    "parameters": {
		"camera_name": "GST_TEST"
	    }
	},
	{
	    "processor_name": "Transformer",
	    "processor_type": "ImageTransformer",
	    "parameters": {
		"channel": "3",
		"width": "227",
		"height": "227",
		"subtract_mean": "false"
	    },
            "inputs": {
                "input": "Camera:bgr_output"
            }
	},
        {
	    "processor_name": "Classifier",
	    "processor_type": "ImageClassifier",
	    "parameters": {
		"model": "Inception",
		"channel": "3",
                "batch_size": "1"
	    },
            "inputs": {
                "input0": "Transformer:output"
            }
	},
	{
	    "processor_name": "Writer",
	    "processor_type": "FileWriter",
	    "parameters": {
		"filename": "/home/ccanel/Desktop/classifier_output"
	    },
            "inputs": {
                "input": "Classifier:output0"
            }
	}

    ]
}
