{
  "pipeline_name": "ObjectClassifier",
  "processors": [{
      "processor_name": "Camera",
      "processor_type": "Camera",
      "parameters": {
        "camera_name": "GST_TEST"
      }
    },
    {
      "processor_name": "Transformer",
      "processor_type": "ImageTransformer",
      "parameters": {
        "channel": "3",
        "width": "299",
        "height": "299",
        "subtract_mean": "false"
      },
      "inputs": {
        "input": "Camera:output"
      }
    },
    {
      "processor_name": "Classifier",
      "processor_type": "ImageClassifier",
      "parameters": {
        "model": "inception",
        "num_channels": "3",
        "num_labels": "5"
      },
      "inputs": {
        "input": "Transformer:output"
      }
    }
  ]
}
