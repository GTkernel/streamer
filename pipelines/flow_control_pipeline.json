{
  "pipeline_name": "FlowControl",
  "processors": [{
      "processor_name": "Camera",
      "processor_type": "Camera",
      "parameters": {
        "camera_name": "GST_TEST"
      }
    },
    {
      "processor_name": "Entrance",
      "processor_type": "FlowControlEntrance",
      "parameters": {
        "max_tokens": "50"
      },
      "inputs": {
        "input": "Camera:output"
      }
    },
    {
      "processor_name": "FirstThrottler",
      "processor_type": "Throttler",
      "parameters": {
        "fps": "10"
      },
      "inputs": {
        "input": "Entrance:output"
      }
    },
    {
      "processor_name": "Exit",
      "processor_type": "FlowControlExit",
      "parameters": {
      },
      "inputs": {
        "input": "FirstThrottler:output"
      }
    },
    {
      "processor_name": "SecondThrottler",
      "processor_type": "Throttler",
      "parameters": {
        "fps": "5"
      },
      "inputs": {
        "input": "Exit:output"
      }
    }
  ]
}
