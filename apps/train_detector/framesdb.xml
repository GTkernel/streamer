<?xml version="1.0"?>
<!DOCTYPE database SYSTEM "litesql.dtd">
<database name="FramesDB">
  <object name="FrameEntry">
    <field name="captureTimeMicros" type="bigint"/>
    <field name="dir" type="string"/>
    <field name="jpegPath" type="string"/>
    <field name="compressionType" type="string"/>
    <field name="exposure" type="float"/>
    <field name="sharpness" type="float"/>
    <field name="brightness" type="float"/>
    <field name="saturation" type="float"/>
    <field name="hue" type="float"/>
    <field name="gain" type="float"/>
    <field name="gamma" type="float"/>
    <field name="wbred" type="float"/>
    <field name="wbblue" type="float"/>
  </object>
</database>
